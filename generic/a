    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script>
        function generateRestReport() {
            let ip = document.getElementById('restIpInput').value;
            if (ip == "") {
                swal("Wait a sec!", "IP can't be empty", "error");
                return;
            }
            let center = document.getElementById('restCentr');
            let p = document.createElement('p');
            p.innerHTML = "Once the reports are generated, the page will contain new links. Please stay patient.";
            center.appendChild(p);
            let div = document.createElement('div');
            div.setAttribute('class', 'loader');
            center.appendChild(div);
            fetch('/ccbd/studentViewing/' + ip).then(response => response.json()).then(res => {
                if (!res.success) {
                    swal("Something went wrong!", "Check if the IP is correct, and try again.", "error");
                } else {
                    swal("Generated!", "You will be redirected to the report.", "success");
                    let data = res.data;
                    document.write(data);
                }
            });
        }

        function generateContainerReport() {
            let ip = document.getElementById('ipInput').value;
            if (ip == "") {
                swal("Wait a sec!", "IP can't be empty", "error");
                return;
            }
            let username = document.getElementById('usernameInput').value;
            if (username == "") {
                swal("Wait a sec!", "Username can't be empty", "error");
                return;
            }
            let privateKeyInput = document.getElementById('privateKeyInput');
            if (privateKeyInput.files.length == 0) {
                swal("Wait a sec!", "Have to provide a private key .pem file", "error");
                return;
            }
            let data = new FormData();
            data.append('privateKey', privateKeyInput.files[0]);
            let center = document.getElementById('containerCentr');
            let p = document.createElement('p');
            p.innerHTML = "Once the reports are generated, the page will contain new links. Please stay patient.";
            center.appendChild(p);
            let div = document.createElement('div');
            div.setAttribute('class', 'loader');
            center.appendChild(div);

            fetch('/ccbd/studentViewing/' + ip + '/' + username, {
                method: 'POST',
                body: data
            }).then(response => response.json()).then(res => {
                if (!res.success) {
                    swal("Something went wrong!", "Check if the credentials are correct, and try again.", "error");
                } else {
                    swal("Generated!", "You will be redirected to the report.", "success");
                    let data = res.data;
                    document.write(data);

                }
            });
        }

        function generateLbReport() {
            let lbDns = document.getElementById('lbDnsInput').value;
            let actsIp = document.getElementById('lbActsIpInput').value;
            let usersIp = document.getElementById('lbUsersIpInput').value;
            let actsUsername = document.getElementById('lbActsUsernameInput').value;
            let usersUsername = document.getElementById('lbUsersUsernameInput').value;
            let actsPkeyInput = document.getElementById('lbActsPkeyInput');
            let usersPkeyInput = document.getElementById('lbUsersPkeyInput');

            if (lbDns == "" || actsIp == "" || usersIp == "" || actsUsername == "" || usersUsername == "" || actsPkeyInput.files.length == 0 || usersPkeyInput.files.length == 0) {
                swal("Wait a sec!", "None of the inputs are allowed to be empty", "error");
                return
            }

            let data = new FormData();
            data.append('actsPrivateKey', actsPkeyInput.files[0]);
            data.append('usersPrivateKey', usersPkeyInput.files[0]);
            let center = document.getElementById('lbCentr');
            let p = document.createElement('p');
            p.innerHTML = "Once the reports are generated, the page will contain new links. Please stay patient.";
            center.appendChild(p);
            let div = document.createElement('div');
            div.setAttribute('class', 'loader');
            center.appendChild(div);

            fetch('/ccbd/studentViewing/' + lbDns + '/' + actsIp + '/' + actsUsername + '/' + usersIp + '/' + usersUsername, {
                method: 'POST',
                body: data
            }).then(response => response.json()).then(res => {
                if (!res.success) {
                    swal("Something went wrong!", "Check if the credentials are correct, and try again.", "error");
                } else {
                    swal("Generated!", "You will be redirected to the report.", "success");
                    let data = res.data;
                    document.write(data);

                }
            });
        }

        function generateOrchReport() {
            let orchSelfieLessActsIp = document.getElementById('orchSelfieLessActsIpInput').value;
            let orchActsIp = document.getElementById('orchActsIpInput').value;
            let orchActsUsername = document.getElementById('orchActsUsernameInput').value;
            let orchActsPkeyInput = document.getElementById('orchActsPkeyInput');

            if (orchSelfieLessActsIp == "" || orchActsIp == "" || orchActsUsername == "" || orchActsPkeyInput.files.length == 0) {
                swal("Wait a sec!", "None of the inputs are allowed to be empty", "error");
                return
            }

            let data = new FormData();
            data.append('actsPrivateKey', orchActsPkeyInput.files[0]);
            let center = document.getElementById('orchCentr');
            let p = document.createElement('p');
            p.innerHTML = "Once the reports are generated, the page will contain new links. Please stay patient, orchestration reports specially take a few minutes.";
            center.appendChild(p);
            let div = document.createElement('div');
            div.setAttribute('class', 'loader');
            center.appendChild(div);

            fetch('/ccbd/studentViewing/' + orchSelfieLessActsIp + '/' + orchActsIp + '/' + orchActsUsername, {
                method: 'POST',
                body: data
            }).then(response => response.json()).then(res => {
                if (!res.success) {
                    swal("Something went wrong!", "Check if the credentials are correct, and try again.", "error");
                } else {
                    swal("Generated!", "You will be redirected to the report.", "success");
                    let data = res.data;
                    document.write(data);
                }
            });
        }

        function generateLink(link) {
            let a = document.createElement('a');
            a.setAttribute('href', link);
            a.setAttribute('target', '_blank');
            a.click();
        }
    </script>
</head>


<body>
    <center>
        <h1>Cloud Computing Student Report Viewing Platform</h1>
    </center>
    <div id="lb">
        <h2>Generic Load Balancer Report</h2>
        <p>Enter your load balancer DNS: </p><input type="text" id="lbDnsInput"><br>
        <p>Enter your `microservice` IP: </p><input type="text" id="lbActsIpInput"><br>
        <p>Enter your `microservice` username: </p><input type="text" id="lbActsUsernameInput"><br>
        <p>Upload your `microservice` private key file: </p><input type="file" id="lbActsPkeyInput" name="privateKey"><br>
        <center id="lbCentr"><button id="genlbbtn" onclick="generateLbReport();">Generate Load Balancer Report</button>
        </center>
    </div>
